{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "config"
      ],
      "properties": {
        "config": {
          "type": "object"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "merkle_root"
      ],
      "properties": {
        "merkle_root": {
          "type": "object",
          "required": [
            "stage"
          ],
          "properties": {
            "stage": {
              "type": "integer",
              "format": "uint8",
              "minimum": 0.0
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "latest_stage"
      ],
      "properties": {
        "latest_stage": {
          "type": "object"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "is_claimed"
      ],
      "properties": {
        "is_claimed": {
          "type": "object",
          "required": [
            "address",
            "stage"
          ],
          "properties": {
            "address": {
              "$ref": "#/definitions/HumanAddr"
            },
            "stage": {
              "type": "integer",
              "format": "uint8",
              "minimum": 0.0
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "HumanAddr": {
      "type": "string"
    }
  }
}
