{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "VotersResponse",
  "type": "object",
  "required": [
    "voters"
  ],
  "properties": {
    "voters": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/VotersResponseItem"
      }
    }
  },
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "Uint128": {
      "type": "string"
    },
    "VoteOption": {
      "type": "string",
      "enum": [
        "yes",
        "no",
        "abstain"
      ]
    },
    "VotersResponseItem": {
      "type": "object",
      "required": [
        "balance",
        "vote",
        "voter"
      ],
      "properties": {
        "balance": {
          "$ref": "#/definitions/Uint128"
        },
        "vote": {
          "$ref": "#/definitions/VoteOption"
        },
        "voter": {
          "$ref": "#/definitions/HumanAddr"
        }
      }
    }
  }
}
